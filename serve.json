{
  "headers": [
    {
      "source": "**",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ],
  "rewrites": [
    { "source": "/components/dist/:file", "destination": "/packages/components/dist/:file" }
  ],
  "unlisted": ["CNAME", ".*", "node_modules", "*.yaml", "*.json", "*.js", "*.css", "yarn.lock"]
}
