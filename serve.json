{
  "headers": [
    {
      "source": "**",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ],
  "rewrites": [
    { "source": "/packages/:space/assets/:file([^/]+\\.[^/]+)", "destination": "/packages/:space/docs/assets/:file" },
    { "source": "/packages/:space/assets/:dir/:file([^/]+\\.[^/]+)", "destination": "/packages/:space/docs/assets/:dir/:file" },
    { "source": "/packages/:space/:file([^/]+\\.[^/]+)", "destination": "/packages/:space/docs/:file" }
  ]
}
